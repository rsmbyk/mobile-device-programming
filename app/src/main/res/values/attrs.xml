<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="pivot" format="enum" >
        <enum name="width" value="0" />
        <enum name="height" value="1" />
        <enum name="smallest" value="2" />
        <enum name="largest" value="3" />
    </attr>

    <declare-styleable name="SquareImageView" >
        <attr name="pivot" />
    </declare-styleable>

    <declare-styleable name="SquareLayout" >
        <attr name="pivot" />
    </declare-styleable>
</resources>
